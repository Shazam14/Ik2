<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DBE3E3"
    android:gravity=""
    android:orientation="vertical"
    android:weightSum="1"
    tools:context=".VerifyData">

    <android.support.v7.widget.Toolbar
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/ikode_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimaryDark"
        android:theme="@style/Theme.AppCompat.Light"
        app:title="Ikode SC Reader"
        app:titleTextColor="#DBE3E3" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="63dp">

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:src="@drawable/globe_splash" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/imageView13"
            android:layout_toRightOf="@+id/imageView13"
            android:src="@drawable/license1" />
    </RelativeLayout>

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/editDataId"
        android:layout_alignStart="@+id/editDataId"
        android:layout_below="@+id/linearLayout8"
        android:text="Data ID" />

    <EditText
        android:id="@+id/editDataId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView22"
        android:enabled="false"
         />
    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editDataId"
        android:text="Data Type" />

    <EditText
        android:id="@+id/editDataType"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"


        android:layout_below="@+id/textView23"
        android:enabled="false"
         />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Description"
        />

    <EditText
        android:id="@+id/editDesc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:enabled="false"
         />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Version" />

    <EditText
        android:id="@+id/editVer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:enabled="false"
       />




    <RelativeLayout
        android:layout_width="100dp"
        android:layout_height="100dp">


        <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:visibility="visible"
            />

    </RelativeLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100dp">

        <Button
            android:id="@+id/saveButt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/button_save"
            android:visibility="invisible" />

        <Button
            android:id="@+id/settButt"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/saveButt"
            android:background="@drawable/button_sets"
            android:visibility="invisible" />
    </RelativeLayout>



</LinearLayout>
